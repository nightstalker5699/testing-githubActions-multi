{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "nightstalker5699/client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "nightstalker5699/backend",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "nightstalker5699/worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "nightstalker5699/nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostport": 80,
          "containerport": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}
